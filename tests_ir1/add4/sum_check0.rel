version 1.0.0;
field characteristic 97 degree 1;
relation
gate_set: arithmetic;
features: @function;
@begin
  @function(add_4, @out: 0, @in: 4, @instance: 0, @short_witness: 0)
    $4 <- @add($0, $1);
    $5 <- @add($2, $3);
    $6 <- @add($4, $5);
    @assert_zero($6);
  @end

  $0 <- @short_witness;
  $1 <- @instance;
  $2 <- @short_witness;
  $3 <- @instance;
  @call(add_4, $0...$3);
@end

